local w = library:CreateWindow("Buy stuff") -- Creates the window

local b = w:CreateFolder("Gun stuff") -- Creates the folder(U will put here your buttons,etc)


b:Dropdown("Guns",{"Hi-Point .380","Block 17","Ruger 9mm","Springfield XDS .45 ACP","Taurus .380 Special","Smith & Wesson 9mm"},true,function(mob) --true/false, replaces the current title "Dropdown" with the option that t
	local args = {
		[1] = "BuyFirearm",
		[2] = mob
	 }
	 
	 game:GetService("ReplicatedStorage").Events.NPCService:FireServer(unpack(args))(mob)
end)

b:Dropdown("Ammo",{"BuyAmmo_380","BuyAmmo_9","BuyAmmo_45"},true,function(mob) --true/false, replaces the current title "Dropdown" with the option that t
	local args = {
		[1] = mob
	 }
	 
	 game:GetService("ReplicatedStorage").Events.ClientService:FireServer(unpack(args))(mob)
end)

local b = w:CreateFolder("Extra stuff") -- Creates the folder(U will put here your buttons,etc)

b:Button("Filtank",function()
-- Script generated by SimpleSpy - credits to exx#9394

local args = {
	[1] = "FillTank"
 }
 
 game:GetService("ReplicatedStorage").Events.VehicleService:FireServer(unpack(args))
end)

