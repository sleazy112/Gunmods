local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3')))()

local w = library:CreateWindow("main") -- Creates the window

local b = w:CreateFolder("main") -- Creates the folder(U will put here your buttons,etc)

b:Button("Godmode",function()
	game.Players.LocalPlayer.Character.FakeHumanoid:Remove()
	local NewInstance = Instance.new("IntValue") --only so that press shift to run can work
	NewInstance.Value = 100 
	NewInstance.Name = "FakeHumanoid"
	NewInstance.Parent = game.Players.LocalPlayer.Character
end)

b:Button("ViewJobs",function()
	local args = {
		[1] = "GiveGui"
	}
	
	game:GetService("ReplicatedStorage").Events.JobService:FireServer(unpack(args))
	
end)

b:Button("Unnamed esp",function()
   loadstring(game:HttpGet("https://raw.githubusercontent.com/ic3w0lf22/Unnamed-ESP/master/UnnamedESP.lua"))();
end)

b:Button("Inf yield",function()
   loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

local b = w:CreateFolder("Player") -- Creates the folder(U will put here your buttons,etc)

b:Toggle("Noclip",function(bool)
    shared.toggle = bool
    noclip = bool
    game:GetService('RunService').Stepped:connect(function()
    if noclip then
    game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
    end
    end)(shared.toggle)
end)

b:Slider("Walkspeed",{
    min = 8; -- min value of the slider
    max = 200; -- max value of the slider
    precise = true; -- max 2 decimals
},function(value)
	game:GetService('Players').LocalPlayer.Character.Humanoid.WalkSpeed = value
end)

b:Slider("Jumppower",{
    min = 30; -- min value of the slider
    max = 200; -- max value of the slider
    precise = true; -- max 2 decimals
},function(value)
	game:GetService('Players').LocalPlayer.Character.Humanoid.JumpPower = value
end)

local w = library:CreateWindow("GunMods") -- Creates the window

local b = w:CreateFolder("Hi-Point") -- Creates the folder(U will put here your buttons,etc)

b:Box("Gundmg","number",function(value) -- "number" or "string"
        -- Generated by Hydroxide's Upvalue Scanner: https://github.com/Upbolt/Hydroxide

local aux = loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/Upbolt/Hydroxide/revision/ohaux.lua"))()

local scriptPath = game:GetService("Players").LocalPlayer.Backpack["Hi-Point .380"].Main
local closureName = "GunEffectShoot"
local upvalueIndex = 15
local closureConstants = {
	[1] = "CheckIfSitting",
	[2] = true,
	[3] = "Barrel",
	[4] = "WaitForChild",
	[5] = "Transparency",
	[6] = "BarrelBack"
}

local closure = aux.searchClosure(scriptPath, closureName, upvalueIndex, closureConstants)
local value = value


-- DO NOT RELY ON THIS FEATURE TO PRODUCE 100% FUNCTIONAL SCRIPTS
debug.setupvalue(closure, upvalueIndex, value)(value)
end)

